{
  "name": "istants-pic",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/stefanoauciello/istants-pic.git",
  "license": "MIT",
  "scripts": {
    "start": "yarn docker-up && yarn wait-on -d 30000 tcp:3306 && node src/index.js",
    "stop": "yarn docker-down",
    "lint": "eslint",
    "delay": "node ./node_modules/npm-delay 45000",
    "test": "yarn docker-up && yarn delay && yarn test:package && yarn docker-down",
    "test:package": "jest --runInBand --forceExit",
    "docker-up": "docker-compose up -d",
    "docker-down": "docker-compose down"
  },
  "dependencies": {
    "amqplib": "^0.7.1",
    "body-parser": "^1.19.0",
    "cron": "^1.8.2",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "formidable": "^1.2.2",
    "mysql2": "^2.2.5",
    "sharp": "^0.27.2",
    "validator": "^13.5.2",
    "npm-delay": "^1.0.4"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "wait-on": "^5.3.0"
  }
}
